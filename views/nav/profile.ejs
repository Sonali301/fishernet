<% layout("/layouts/boilerplate") %>
<!-- Base styles for interactive background -->
<link rel="stylesheet" href="css/background.css">

<!-- Main page styles -->
<link rel="stylesheet" href="css/main.css">

<!-- Projects page specific styles -->
<link rel="stylesheet" href="css/profile.css">

<!-- Canvas for interactive background -->
<canvas id="canvas"></canvas>


<!-- Main content -->
<div class="content projects-content">
    <h1>The Heart of Tamil Nadu's Coast</h1>

    <div class="projects-intro">
        <p>
            With weathered hands and eyes that read the sea, the Tamil Nadu fisherman embodies resilience, navigating
            both the ocean's depths and the currents of change in pursuit of their livelihood.
        </p>
    </div>

    <!-- Projects Quick Overview Section -->

    <!-- Projects Detailed Section -->
    <div class="projects-detailed">
        <h2>User Profile</h2>

        <div class="project-card" id="request-interceptor">
            <div class="project-content">
                <div class="project-header">
                    <h3>Personal Infromation</h3>

                </div>
                <div class="project-description">

                    <img src="data:image/png;base64,"
                        alt="Profile Picture" class="profile-image">
                    <p><strong>Name:</strong> John Doe</p>
                    <p><strong>Fishing License Number:</strong> 123456</p>
                    <p><strong>Contact Number:</strong> +91 9876543210</p>

                </div>
            </div>
        </div>

        <div class="project-card" id="bgpulse">
            <div class="project-content">
                <div class="project-header">
                    <h3>Boat Information</h3>

                </div>
                <div class="project-description">
                    <p>

                        <img src="data:image/jpeg;base64,"
                            alt="Boat Picture" class="profile-image">
                    <p><strong>Boat Name:</strong> Sea Breeze</p>
                    <p><strong>Boat Registration Number:</strong> TN-123</p>
                    <p><strong>Boat Type:</strong> Mechanized Boat</p>
                </div>
            </div>
        </div>

        <div class="project-card" id="headerguard">
            <div class="project-content">
                <div class="project-header">
                    <h3>Fishing Gear</h3>

                </div>
                <div class="project-description">

                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRT_43iranVZQEBhC3iDb0Ob9qr6HUpRtbEww&s"
                        alt="Fishing Gear" class="profile-image">
                    <p><strong>Net Type:</strong> Gill Net</p>
                    <p><strong>Hook Type:</strong> Longline Hooks</p>
                </div>
            </div>
        </div>

        <div class="project-card" id="netgazer">
            <div class="project-content">
                <div class="project-header">
                    <h3>Settings</h3>
                    <div class="project-links">
                        <a href="https://github.com/5u5urrus/NetGazer" class="text-link">GitHub</a>
                    </div>
                </div>
                <div class="project-description">

                    <img src="data:image/jpeg;base64,"
                        alt="Settings" class="profile-image">
                    <p><strong>Notifications:</strong> Enabled</p>
                    <p><strong>Language:</strong> English</p>

                </div>
            </div>
        </div>

        <!-- New project cards for added tools -->

    </div>

    <!-- More Projects Coming Soon Section -->
    <div class="more-projects">
        <h2>Catch Report</h2>
        <div id="catch-report">
            <form id="catch-form">
                <label for="species">Species:</label>
                <input type="text" id="species" name="species" required>
                <label for="quantity">Quantity (kg):</label>
                <input type="number" id="quantity" name="quantity" required>
                <button type="submit">Submit Catch</button>
            </form>
        </div>
    </div>
</div>

<!-- Import the interactive background script -->
<script src="js/interactive-background.js"></script>

<!-- Projects page specific script -->
<script src="js/projects.js"></script>

<!-- Initialize the interactive background -->
<script>
    document.addEventListener('DOMContentLoaded', function () {
        // Initialize the interactive background
        new InteractiveBackground('canvas');

        // Smooth scroll to project details when clicking on overview cards
        document.querySelectorAll('.overview-card').forEach(card => {
            card.addEventListener('click', function () {
                const target = this.getAttribute('data-target');
                const element = document.getElementById(target);
                if (element) {
                    element.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });
    });
</script>